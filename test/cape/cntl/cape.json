{
    // Compute job PBS options
    "PBS": {
        "j": "oe",
        "r": "n",
        "S": "/bin/bash",
        "select": 10,
        "ncpus": 28,
        "mpiprocs": 28,
        "model": "bro",
        "W": "group_list=e0847",
        "q": "sls_aero1",
        "walltime": "24:00:00"
    },
    // Post-processing PBS options
    "BatchPBS": {
        "q": "sls_aero1",
        "select": 1,
        "walltime": "8:00:00",
        "model": "has",
        "ncpus": 24,
        "mpiprocs": 24
    },

    // Commands to run at beginning of PBS scripts
    "ShellCmds": [
        ". $MODULESHOME/init/bash",
        "module use -a /home5/ddalle/share/modulefiles",
        "module use -a /home3/fun3d/shared/n1337/modulefiles",
        "module load FUN3D/13.1",
        "module load mpi-sgi/mpt.2.15r20",
        "module load aflr3",
        "module load python/2.7.12",
        "module load pycart/devel"
    ],
    // Test including another JSON file
    JSONFile("BatchShell.json"),
    
    // Permissions options
    "umask": "0027",
    
    // Module settings
    "PythonPath": ["tools"],
    "Modules": ["dac"],
    "InitFunction": ["dac.InitCntl"],
    "CaseFunction": ["dac.ApplyTag"],
    
    // Options for overall run control and command-line inputs
    "RunControl": {
        // Run sequence
        "PhaseSequence": [0],
        "PhaseIters": [200],
        // Operation modes
        "nProc": 4,
        "MPI": false
	},

	// Parameters for creating the mesh
    "Mesh": {
        // This can be a list of .tri files.
        "TriFile": "bullet.tri"
    },
    
    // Describe the reference values and config.
    "Config": {
        // Defer to a file for most things.
        "File": "Config.xml",
        // Which forces should be reported
        "Components": ["cap", "body", "bullet_no_base", "bullet_total"],
        // Reference values
        "RefArea": 3.14159,
        "RefLength": 1.0,
        // The moment point can be specified as a dictionary of components.
        "RefPoint": {"bullet_no_base": [0.0, 0.0, 0.0]}
    },
    
    // Trajectory description
    "Trajectory": {
        // If a file is specified, and it exists, trajectory values will be
        // read from it.  Trajectory values can also be specified locally.
        "File": "matrix.csv",
        "Keys": ["mach", "alpha", "beta", "tag"],
        // Copy the mesh
        "GroupMesh": true,
        // Configuration name [default]
        "GroupPrefix": "poweroff",
        // Specific definitions for custom keys
        "Definitions": {
            "tag": {
                "Type": "value",
                "Value": "str",
                "Label": false,
                "Group": false
            }
        }
    }
}
